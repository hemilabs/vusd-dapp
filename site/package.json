{
  "name": "vusd-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "deps:check": "dependency-check --detective precinct --ignore-module autoprefixer --ignore-module patch-package --ignore-module react-dom --ignore-module react --ignore-module tailwindcss --ignore-module cors --ignore-module next-secure-headers --no-dev pages/*",
    "prestart": "npm run build",
    "start": "next start"
  },
  "dependencies": {
    "@sentry/nextjs": "7.120.4",
    "@web3-react/core": "6.1.9",
    "@web3-react/injected-connector": "6.0.7",
    "@web3-react/walletconnect-connector": "6.2.13",
    "@web3-react/walletlink-connector": "6.2.14",
    "autoprefixer": "10.4.21",
    "big.js": "6.2.2",
    "cors": "2.8.5",
    "next": "11.1.4",
    "next-secure-headers": "2.2.0",
    "next-translate": "1.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "0.5.7",
    "tailwindcss": "2.2.19",
    "vusd-lib": "1.0.0",
    "@hemilabs/wallet-watch-asset": "1.0.2",
    "web3": "1.10.4"
  },
  "babel": {
    "presets": [
      [
        "next/babel",
        {
          "preset-env": {
            "useBuiltIns": false,
            "targets": "defaults, not ie 11"
          }
        }
      ]
    ]
  }
}
